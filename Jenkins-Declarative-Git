pipeline {
    agent any
    
    tools {
        maven '3.6.3'
    }
    environment {
        PATH="/opt/apache-maven-3.6.3/bin:$PATH"
    }
    stages{
        stage("Git Checkout"){
            steps{
                git credentialsId: 'github-private-key', url: 'https://github.com/thenoise2000/myweb'
            }
        }
        stage("Example"){
            steps{
                sh "mvn --version"
            }
        }
    }
}
