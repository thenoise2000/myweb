pipeline {
    agent any
    
    tools {
        maven '3.6.3'
    }
    environment {
        JAVA_HOME='C:\Program Files (x86)\Java\jdk1.8.0_144'
    }
    stages{
        stage("Git Checkout"){
            steps{
                git credentialsId: 'github-private-key', url: 'https://github.com/thenoise2000/myweb'
            }
        }
        stage("Example"){
            steps{
                sh "mvn --version"
            }
        }
    }
}
