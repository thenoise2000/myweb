pipeline {
    agent any
    
    environment{
        PATH = "C:\maven\bin:$PATH"
    }
    stages{
        stage("Git Checkout"){
            steps{
                git credentialsId: 'github-private-key', url: 'https://github.com/thenoise2000/myweb'
            }
        }
        stage("Maven Build"){
            steps{
                sh "mvn clean package"
            }
        }
    }
}
