pipeline {
    agent any
    
    tools {
        maven "3.6.3"        
    }
    environment {
        PATH = ${MAVEN_HOME}\bin:${PATH}
        MAVEN_HOME = C:\maven
    }
    stages{
        stage("Git Checkout"){
            steps{
                git credentialsId: 'github-private-key', url: 'https://github.com/thenoise2000/myweb'
            }
        }
        stage("Example"){
            steps{
                sh "mvn --version"
            }
        }
    }
}
